{
  "name": "1gram",
  "version": "0.11.1",
  "description": "FreeTON Wallet",
  "scripts": {
    "build": "npm run browser:build",
    "lint": "vue-cli-service lint",
    "analyze": "source-map-explorer 'dist/js/*.js' --no-border-checks",
    "browser:build": "yarn vue-cli-service build && cp ./dist/index.html ./dist/200.html",
    "browser:start": "yarn vue-cli-service serve",
    "deploy:production": "firebase deploy --only hosting",
    "deploy:production:ci": "firebase deploy --only hosting --token $FIREBASE_TOKEN",
    "deploy:staging": "firebase hosting:staging:deploy --expires 30d",
    "deploy:staging:ci": "firebase hosting:staging:deploy --expires 30d --token $FIREBASE_TOKEN",
    "deploy:surge": "surge ./dist --domain https://1gram.surge.sh",
    "deploy:surge:ci": "surge ./dist --domain https://1gram.surge.sh --token $SURGE_TOKEN",
    "electron:build": "vue-cli-service electron:build",
    "electron:install": "electron-builder install-app-deps",
    "electron:serve": "vue-cli-service electron:serve",
    "prebuild": "cp ./node_modules/ton-client-web-js/tonclient.wasm ./public/tonclient.wasm",
    "start": "npm run browser:start",
    "test": "ts-mocha -p tsconfig.json src/__tests__/**/*.ts --timeout 150000"
  },
  "main": "background.js",
  "dependencies": {
    "@alch/alchemy-web3": "^0.1.18",
